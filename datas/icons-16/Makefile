install:
	$(MKINSTALLDIRS) $(icons16dir);\
	for f in $(ICONS16); do\
		echo "Installing $(icons16dir)/$$f";\
		$(INSTALL_DATA) $$f $(icons16dir);\
	done

uninstall:
	$(RM) -r $(icons16dir)

clean:
