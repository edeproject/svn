install:
	$(MKINSTALLDIRS) $(icons48dir);\
	for f in $(ICONS48); do\
		echo "Installing $(icons48dir)/$$f";\
		$(INSTALL_DATA) $$f $(icons48dir);\
	done

uninstall:
	$(RM) -r $(icons48dir)

clean:
